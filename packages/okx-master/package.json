{
  "name": "okx-master",
  "private": true,
  "version": "0.0.1",
  "description": "A template for creating npm packages using TypeScript and VSCode",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/mjs/*.d.ts",
        "default": "./dist/mjs/*.js"
      },
      "require": {
        "types": "./dist/cjs/*.d.ts",
        "default": "./dist/cjs/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "prebuild": "npm run clean",
    "build:server": "tsc --project tsconfig.cjs.json",
    "clean": "rm -rf ./lib/ && rm -rf ./dist/",
    "eslint": "eslint ./src/ --fix",
    "lintpackagejson": "npmPkgJsonLint .",
    "release": "npx changeset publish",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "debug": "nodemon --watch \"src/server/**\" --watch script --exec node --inspect=9231 -r ts-node/register ./script/start.ts",
    "dev": "nodemon --watch \"src/server/**\" --watch script --files ./script/start.ts",
    "dev:client": "vite",
    "dev:server": "nodemon --watch \"src/server/**\" --exec node --inspect=9231 -r ts-node/register ./src/server/server.ts",
    "start": "node ./dist/server.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitlint",
    "changesets",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/hsuehic/okx-node/issues"
  },
  "homepage": "https://github.com/hsuehic/okx-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsuehic/okx-node.git"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.27",
    "@koa/bodyparser": "^5.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "antd": "^5.9.2",
    "dotenv": "^16.3.1",
    "echarts": "^5.4.3",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "koa": "^2.14.2",
    "koa-passport": "^6.0.0",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "moment": "^2.29.4",
    "okx-node": "workspace:^",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "tsxecharts": "^1.2.2",
    "typeorm": "^0.3.17",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.8",
    "@types/koa-passport": "^6.0.1",
    "@types/koa-session": "^6.4.2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^4.0.1",
    "@types/koa__router": "^12.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "koa-connect": "^2.1.0",
    "nodemon": "^3.0.1",
    "sass": "^1.68.0",
    "ts-node": "^10.2.1",
    "vite": "^4.4.9"
  }
}
