{
  "name": "okx-master",
  "private": true,
  "version": "0.0.0-development",
  "description": "A template for creating npm packages using TypeScript and VSCode",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/mjs/*.d.ts",
        "default": "./dist/mjs/*.js"
      },
      "require": {
        "types": "./dist/cjs/*.d.ts",
        "default": "./dist/cjs/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:mjs": "tsc --project tsconfig.mjs.json",
    "clean": "rm -rf ./lib/ && rm -rf ./dist/",
    "eslint": "eslint ./src/ --fix",
    "lintpackagejson": "npmPkgJsonLint .",
    "release": "npx changeset publish",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "dev": "vite"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitlint",
    "changesets",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/hsuehic/okx-node/issues"
  },
  "homepage": "https://github.com/hsuehic/okx-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsuehic/okx-node.git"
  },
  "dependencies": {
    "@koa/bodyparser": "^5.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "koa": "^2.14.2",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "okx-node": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.8",
    "@types/koa__router": "^12.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "vite": "^4.4.9"
  }
}
